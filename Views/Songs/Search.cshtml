@model IEnumerable<SpotifyApp.Models.Song>


@{
    ViewData["Title"] = "Search";
}
<form asp-controller="Songs" asp-action="SearchSong">
    <p>
        Title: <input type="text" name="searchItem" />
        <input type="submit" value="Search" />
    </p>
</form>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ArtistName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SongName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ArtistName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SongName)
                </td>
            </tr>
        }
    </tbody>
</table>